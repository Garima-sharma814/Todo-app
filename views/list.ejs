<%- include("header") -%>
  <div class="box" id="heading">
    <h1>
      <%=listName%>
    </h1>
  </div>
  <div class="box">
    <% for(var i=0; i<addeditems.length; i++){ %>
      <% if(addeditems[i]==="" ){ %>
        <% console.log("Please add something") %>
          <% }else{ %>
            <form action="/delete" method="POST">
            <div class="item">
              <input type="checkbox" name="checkbox" onChange="this.form.submit()"  value="<%=addeditems[i]._id%>" >
              <p>
                <%= addeditems[i].name %>
              </p>
            </div>
          </form>
            <% } %>
              <% } %>
                <form class="item" action="/" method="post">
                  <input type="text" name="newItem" placeholder="Enter New Item" autocomplete="off">
                  <button type="submit" name="list" value=<%=listName %>>+</button>
                </form>
  </div>
  <%- include("footer") -%>